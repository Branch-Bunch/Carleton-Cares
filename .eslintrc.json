{
  "extends": "airbnb",
  "installedESLint": true,
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "semi": ["error", "never"],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "default-case": "off"
  }
}
